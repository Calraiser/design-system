    border-bottom: 1px dotted #ccc;
    padding: 10px 0 20px;
    margin-bottom: 20px;
    float: left;
    width: 50%
  }

  .preview-glyphs { vertical-align: bottom; }

  .preview-scale {
    color: #888;
    font-size: 12px;
    margin-top: 5px;
  }

  .icons-preview-step {
    display: inline-block;
    line-height: 1;
    position: relative;
    width: 13%;
  }

  .icons-preview-step .letters,
  .icons-preview-step i {
    -webkit-transition: opacity .3s;
    -moz-transition: opacity .3s;
    -ms-transition: opacity .3s;
    -o-transition: opacity .3s;
    transition: opacity .3s;
  }

  .icons-preview-step:hover .letters { opacity: 1; }
  .icons-preview-step:hover i { opacity: .3; }

  .letters {
    opacity: .3;
    position: absolute;
  }

  .characters-off {
    overflow: hidden;
  }

  .characters-off .letters { display: none; }
  .characters-off .icons-preview-step:hover i { opacity: 1; }

  
  .size-12 { font-size: 12px; }
  
  .size-16 { font-size: 16px; }
  
  .size-21 { font-size: 21px; }
  
  .size-24 { font-size: 24px; }
  
  .size-36 { font-size: 36px; }
  
  .size-48 { font-size: 48px; }
  
