doctype html
html
  head
    = partial 'layouts/meta_tags'
    = stylesheet_link_tag :all
    = javascript_include_tag :all
    = favicon_tag 'favicon-32x32.png'

  body class = page_classes

    .main-content#main
      = partial 'layouts/sidebar'
      .main-content-wrapper
        = partial 'blocks/search'

        .content-wrapper
          = partial 'layouts/subnav'
          = yield
          = partial 'blocks/toTop'
          <script async src="https://www.googletagmanager.com/gtag/js?id=UA-127480418-1"></script>
          = google_analytics_tag
          .visel
